<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:id="@+id/linearLayoutOnePhonebook"
              android:orientation="vertical"
              style="@style/stlPhoneBookListItem"
>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
    >
        <ImageView
                android:id="@+id/imgPhoto"
                style="@style/stlImgPersonNormal"
        />
        <ImageView
                android:id="@+id/imgStart"
                style="@style/stlImgStarPersonList"
                android:src="@android:drawable/star_big_on"
        />
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
        >
            <TextView
                    android:text="TextView"
                    style="@style/stlPhoneBookNameNormal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtName"
            />
            <TextView
                    android:text="TextView"
                    style="@style/stlPhoneBookCompanyNameNormal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtCompanyName"
            />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
